  <br>\n                <button @click=\"purchaseNow(movie)\" class=\"btn btn-primary m-2\">Buy Now</button>\n            </b-card-text>\n        </b-card>\n        <PurchasingModal\n            v-if=\"purchasing\"\n            @close-modal=\"purchasing = false\"\n         />\n    </b-row>\n</template>\n\n<script>\nimport PurchasingModal from './PurchasingModal.vue'\n\nexport default {\n    name: 'Movie',\n    components: {\n        PurchasingModal\n    },\n    data: function() {\n        return {\n            selectedTitle: '',\n            purchasing: false,\n        }\n    },\n    computed: {\n        movies () {\n            return this.$store.getters.getMovies\n        },\n    },\n    methods: {\n        purchaseNow(movie) {\n            this.purchasing = true\n            this.$store.dispatch('purchaseTix', movie)\n        },\n    }\n}\n</script>\n\n<style>\n\n</style>\n"]}]}