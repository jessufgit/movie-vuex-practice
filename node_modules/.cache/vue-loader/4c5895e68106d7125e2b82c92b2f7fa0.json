idth=\"250\"\n            >\n            <b-card-text>\n                <span\n                    v-if=\"movie.awardWinner\">Relevant Awards:<br>\n                    <span v-for=\"award in movie.awardWinner\" :key=\"award.id\">&#127775;{{award}}</span>\n                    <br>\n                </span>\n                <br>\n                <span>Rated: {{movie.rating}}</span>\n                <br>\n                <b-button\n                    v-if=\"movie.available_tickets > 0\"\n                    @click=\"purchaseNow(movie)\"\n                    variant=\"info\"\n                    pill\n                >Buy Now\n                </b-button>\n                <b-button\n                    disabled\n                    variant=\"warning\"\n                    cursor: pointer\n                    v-else>\n                Sold Out\n                </b-button>\n                <br>\n\n            </b-card-text>\n\n        </b-card>\n        <PurchasingModal :movie=\"movie\"\n            v-if=\"purchasing\"\n            @close-modal=\"purchasing = false\"\n         />\n    </b-row>\n</template>\n\n<script>\nimport PurchasingModal from './PurchasingModal.vue'\n\nexport default {\n    name: 'MovieListItem',\n    components: {\n        PurchasingModal\n    },\n    data: function() {\n        return {\n            purchasing: false,\n            movie: {},\n        }\n    },\n    computed: {\n        movies () {\n            return this.$store.getters.getMovies\n        },\n    },\n    methods: {\n        purchaseNow(movie) {\n            this.movie = movie\n            this.purchasing = true\n        },\n    }\n}\n</script>\n\n<style scoped>\nspan {\n    padding: 5px;\n    margin: 10px;\n}\n.soldOut {\n    cursor: pointer;\n}\n.btn-info {\n    margin-top: 10px;;\n}\n</style>\n"]}]}