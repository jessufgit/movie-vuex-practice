put>\n      </b-form-group>\n      <b-form-group\n        label-cols-lg=\"2\"\n        label-for=\"reviewerName\"\n        label=\"Your Name: \"\n      >\n        <b-form-input\n          id=\"reviewerName\"\n          v-model=\"review.id\"\n        >\n        </b-form-input>\n      </b-form-group>\n      <b-form-group\n        label-cols-lg=\"2\"\n        label-for=\"reviewInput\"\n        label=\"Your Review: \"\n      >\n        <b-form-textarea\n          v-model=\"review.reviewText\"\n          id=\"reviewInput\"\n          rows=\"3\"\n          max-rows=\"6\"\n        >\n        </b-form-textarea>\n      </b-form-group>\n      <b-form-group\n        label-cols-lg=\"2\"\n        id=\"ratingBlock\">\n        <star-rating\n          :star-size=\"30\"\n          :glow=2\n          id=\"ratingStars\"\n          active-color='#008080'\n          @rating-selected=\"getRating\"\n        ></star-rating>\n      </b-form-group>\n      <b-button type=\"submit\" @click.prevent=\"addAReview(review)\" variant=\"info\">Submit Review</b-button>\n    </b-form>\n    <div\n      id=\"finalReview\"\n      v-if=\"postReview\">\n      <div>{{review.movieTitle}}</div>\n      <div>{{review.rating}} stars</div>\n      <div>{{review.reviewText}}</div>\n      <div>{{review.id}}</div>\n    </div>\n  </div>\n</template>\n<script>\nimport StarRating from 'vue-star-rating'\n\nexport default {\n  // props: [\n  //   'movie'\n  // ],\n  components: {\n    StarRating\n  },\n  data() {\n    return {\n      passedMovie: false,\n      postReview: false,\n      review: {\n        id: '',\n        reviewText: '',\n        rating: 0,\n        movieTitle: ''\n      },\n    }\n  },\n  created: function() {\n    if(this.$route.params.movie.title != undefined) {\n        this.passedMovie = true;\n        this.review.id = '';\n        this.review.reviewText = '';\n        this.review.rating = 0;\n        this.review.movieTitle = this.$route.params.movie.title\n    }\n  },\n  methods: {\n    getRating: function(rating) {\n      this.review.rating = rating\n    },\n    // addReview: function(currentReview) {\n    //   this.postReview = true\n    //   this.review.id = currentReview.id\n    //   this.review.reviewText = currentReview.reviewText\n    //   this.review.movieTitle = currentReview.movieTitle\n    // },\n    addAReview: function(currentReview) {\n      this.postReview = true\n      this.review = currentReview\n      this.$store.commit('addMovieReview', {title: this.review.id, movieReview: this.review })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n#reviewPage {\n  color: white;\n}\n#reviewForm {\n  width: 80%;\n  margin: 0 auto;\n}\n\n#ratingStars {\n  padding: 5px;\n}\n\n</style>\n"]}]}