                </b-col>\n                    <b-col>\n                        <b-form-group\n                            label=\"Available Times:\"\n                            label-for=\"input-3\">\n                            <b-form-select id=\"input-3\" v-model=\"form.selectedMovieTime\" :options=\"times\"></b-form-select>\n                        </b-form-group>\n                    </b-col>\n                    <b-col>\n                        <b-button variant=\"primary\" id=\"continueButton\" @click.prevent=\"finishPurchase\">Continue</b-button>\n                    </b-col>\n                </b-row>\n                <div v-if=\"readyToPurchase\">\n                    <span>  {{ movie.title }}at {{form.selectedMovieTime}} for {{form.selectedTicketNum}} people?</span>\n                    <b-button slot=\"button\"\n                        variant=\"success\"\n                        @click=\"completeOrder()\">\n                        Buy Tickets Now\n                    </b-button>\n                </div>\n            </b-container>\n        </b-form>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'PurchasingModal',\n  props: [\n    'movie'\n  ],\n  data: function() {\n    return {\n      numOfTix: [\n        {value: 1, text: '1'},\n        {value: 2, text: '2'},\n        {value: 3, text: '3'},\n        {value: 4, text: '4'},\n        {value: 5, text: '5'},\n      ],\n      times: [\n          {value: '12:30', text: '12:30'},\n          {value: '3:00', text: '3:00'},\n          {value: '5:30', text: '5:30'},\n          {value: '8:00', text: '8:00'},\n          {value: '10:30', text: '10:30'},\n      ],\n      readyToPurchase: false,\n      form: {\n        selectedMovieTime: '',\n        selectedTicketNum: 0,\n      },\n    }\n  },\n  computed: {\n\n  },\n  methods: {\n    finishPurchase() {\n      this.readyToPurchase = true\n      this.purchasing = false\n    },\n    completeOrder() {\n      this.$store.commit('purchaseTix', {id: this.movie.id, tickets_requested: this.form.selectedTicketNum})\n      this.$emit('close-modal');\n    }\n  },\n}\n</script>\n\n<style>\n#modal {\n  z-index: 21;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background: rgba(0, 0, 0, 0.6);\n}\n.modal-content {\n  padding: 36px;\n  border-radius: 8px;\n  width: 550px;\n  position: fixed;\n  max-height: 100%;\n  overflow: auto;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  background: #fff;\n}\n.modal-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 36px;\n}\nh2 {\n  font-weight: 600;\n  font-size: 20px;\n}\n.modal-header img {\n  width: 20px;\n  height: 20px;\n}\n.input-field {\n  width: 300px;\n}\n.inline .field {\n  display: inline-block;\n  margin-right: 16px;\n}\n</style>\n"]}]}