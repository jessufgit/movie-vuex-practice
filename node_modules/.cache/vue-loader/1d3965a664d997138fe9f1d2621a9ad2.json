      class=\"btn btn-info\"\n                >Buy Now\n                </b-link>\n                <b-link\n                    v-else\n                    disabled\n                    cursor: pointer\n                    class=\"btn btn-warning\"\n                >\n                Sold Out\n                </b-link>\n                <span\n                    id=\"awardBox\"\n                    v-if=\"movie.awardWinner\">Relevant Awards:<br>\n                    <span v-for=\"award in movie.awardWinner\" :key=\"award.id\">&#127775;{{award}}</span>\n                    <br>\n                </span>\n                <span id=\"rating\">Rated: {{movie.rating}}</span>\n                <span id=\"rating\">Latest Review: {{movie.reviewText}}</span>\n                <b-link\n                    id=\"review\"\n                    :to=\"{name: 'review', params: {movie: movie}}\"\n                    router-tag=\"button\"\n                    class=\"btn btn-primary\"\n                >Review This Movie\n                </b-link>\n            </b-card-text>\n        </b-card>\n    </b-row>\n</template>\n\n<script>\n// import {Movie} from '../../js/movie.js'\nexport default {\n    name: 'MovieListItem',\n    data: function() {\n        return {\n            movie: {},\n        }\n    },\n    computed: {\n        movies () {\n            return this.$store.getters.getMovies\n        },\n    },\n}\n</script>\n\n<style scoped>\n.soldOut {\n    cursor: pointer;\n}\n.movieCard {\n    padding: 5px;\n    margin: 0 auto;\n    background-color: black;\n    color: white;\n    margin-bottom: 2px;\n}\n.movieCard img {\n    max-width: 300px;\n    margin: 0 auto;\n}\n#rating, #awardBox, #purchaseButton, #review {\n    display: block;\n    margin: 10px auto;\n}\n.cardText {\n    text-align: center;\n}\n</style>\n"]}]}